{
    "extends": [
        "airbnb",
        "plugin:flowtype/recommended"
    ],
    "plugins": [
        "flowtype"
    ],
    "rules": {
        "semi": [
            2,
            "never"
        ],
        "no-unexpected-multiline": 2,
        "comma-dangle": 0,
        "no-param-reassign": 0,
        "no-underscore-dangle": 0,
        "no-plusplus": 0
    },
    "env": {
        "browser": true
    }
}